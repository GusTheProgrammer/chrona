{
  "name": "Chrona",
  "dockerComposeFile": "./docker-compose.yml",
  "service": "local-dev",
  "workspaceFolder": "/home/build/app",
  "forwardPorts": [
    3000,
    5432,
    5050
  ],
  "extensions": [
    "dbaeumer.vscode-eslint"
  ],
  "remoteUser": "build",
  "runArgs": [
    "--add-host=host.docker.internal:host-gateway"
  ],
  "postCreateCommand": "git config --global --add safe.directory /home/build/app"
}